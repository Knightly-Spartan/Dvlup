<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>No_Edit_after_prospecting</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL(StageName, &apos;Prospecting&apos;), NOT( ISNEW() ) )</errorConditionFormula>
    <errorMessage>Cannot Edit Prospecting Record</errorMessage>
</ValidationRule>
